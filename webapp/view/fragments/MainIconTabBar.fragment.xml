<core:FragmentDefinition 
    xmlns="sap.m"
    xmlns:core="sap.ui.core" >
    
    <IconTabBar
        id="iconTabBar"
        class="sapUiResponsiveContentPadding"
        headerMode="Inline"
        expandable="false"
        select="onIconTabBarSelect"
        items="{
            path: '/zz_pv_gifts_ckpt_status_count',
            sorter: [{ path: 'sequence', descending: false }]
        }">
        <items>
            <IconTabFilter
                key="{reservationStatus}"
                text="{description}"
                count="{totalRows}"
                icon="{icon}" />
        </items>
    </IconTabBar>

</core:FragmentDefinition>
